<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.board.mapper.FileMapper">

    <!-- 파일 업로드 -->
    <insert id="insertFile" parameterType="FileVo" useGeneratedKeys="true" keyProperty="fileNum">
        INSERT INTO file_ex (
            file_original_name,
            file_saved_name,
            file_path,
            file_created_user
        )
        VALUES (#{fileOriginalName},
                #{fileSavedName},
                #{filePath},
                #{fileCreatedUser})
    </insert>
</mapper>